default: all

TARGETS=name
OPTIM=-O
OBJECTS=name.o
CLEANLIST=test.sysimg test.sysvol
INC=-I$(CAPROS_HEADERS)/include
DOMLIB=$(EROS_ROOT)/lib/$(EROS_TARGET)/libdomain.a

BOOT=$(EROS_ROOT)/lib/$(EROS_TARGET)/image/boot
VOLMAP=../test.volmap

include $(EROS_ROOT)/src/base/lib/make/makerules.mk
include ../test.makeinc

name: name.o $(DOMLIB)
	$(DOMLINK) $(DOMLINKOPT) -o name $(DOMCRT0) name.o $(DOMLIB)

-include .*.m

