/*********************************************
 * PRIMORDIAL DOMAINS
 *********************************************/

#include <primebank.map>
#include <ipltool.map>

extnode = new process;

extnode space = small program "extnode";
extnode pc = symbol "extnode" _start;
extnode schedule = sched(8);

CONSTIT(extnode, KC_OSTREAM, 3) = misc Console;

tree = new node with lss 0;
bigtree = new node with lss 1;

tree[0] = number(0);
tree[1] = number(1);

bigtree[0] = new node with lss 0;
bigtree[1] = new node with lss 0;

bigtree[0][0] = number(0);
bigtree[0][1] = number(1);
bigtree[1][0] = number(0x20);
bigtree[1][1] = number(0x21);

CONSTIT(extnode, KC_OSTREAM, 3) = misc Console;
CONSTIT(extnode, KC_KEYBITS, 4) = misc KeyBits;
CONSTIT(extnode, KC_TREE, 5) = tree;
CONSTIT(extnode, KC_BIGTREE, 6) = bigtree;

extnode key reg KR_SELF = extnode;
extnode key reg KR_BANK = primebank;

run extnode;

print directory;
