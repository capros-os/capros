#include <primebank.map>
#include <pcc.map>
#include <metacon.map>
#include <supernode.map>
#include <vcs.map>

#include <w1mult.map>

w1usbconfig = program segtree LIBDIR "w1usbconfig";
W1Mult(usb, w1usbconfig)
#define ds9490r_notifyee usbw1mult
volsize[13] = threadlist;

#include <nplink.map>

/* test keys */
devNode = new node;
devNode[0] = w1device(usb, 0);
devNode[1] = w1device(usb, 1);
devNode[2] = w1device(usb, 2);
devNode[3] = w1device(usb, 3);
devNode[4] = w1device(usb, 4);
devNode[5] = w1device(usb, 5);
devNode[6] = w1device(usb, 6);
devNode[7] = w1device(usb, 7);
devNode[8] = w1device(usb, 8);
devNode[9] = w1device(usb, 9);
devNode[10] = w1device(usb, 10);
devNode[11] = w1device(usb, 11);
devNode[12] = w1device(usb, 12);
devNode[13] = w1device(usb, 13);
devNode[14] = w1device(usb, 14);
devNode[15] = w1device(usb, 15);
devNode[16] = w1device(usb, 16);
devNode[17] = w1device(usb, 17);
devNode[18] = w1device(usb, 18);
devNode[19] = w1device(usb, 19);
devNode[20] = w1device(usb, 20);
devNode[21] = w1device(usb, 21);
devNode[22] = w1device(usb, 22);
devNode[23] = w1device(usb, 23);
devNode[24] = w1device(usb, 24);
devNode[25] = w1device(usb, 25);
devNode[26] = w1device(usb, 26);
devNode[27] = w1device(usb, 27);
devNode[28] = w1device(usb, 28);
devNode[29] = w1device(usb, 29);

/*
 * Test domain
 */ 
test = new process;

test pc = symbol BUILDDIR "test" _start;
seg = small program BUILDDIR "test";
seg = seg with page at 0x1f000; /* for stack */
test space = seg;
test schedule = sched(8);   /* normal */

test key reg KR_BANK = primebank;
test key reg KR_SCHED = sched(8);

test key reg KR_APP(1) = misc Console;
test key reg KR_APP(2) = misc Sleep;
test key reg KR_APP(4) = devNode;

run test;
