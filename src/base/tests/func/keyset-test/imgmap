#include <primebank.map>
#include <pcc.map>
#include <metacon.map>
#include <vcs.map>
#include <ipltool.map>

#include <keyset.map>

/*
 * Keyset test domain
 */ 
keysettest = new process;

keysettest pc = symbol BUILDDIR "keyset-test" _start;
seg = small program BUILDDIR "keyset-test";
seg = seg with page at 0x1f000; /* for stack */
keysettest space = seg;
keysettest schedule = sched(8);   /* normal */

keysettest key reg 2 = keysettest;
keysettest key reg 4 = primebank;
keysettest key reg 5 = sched(8);

keysettest key reg 6 = keyset_c;
keysettest key reg 8 = misc Console;
keysettest key reg 9 = misc Sleep;

run keysettest;

