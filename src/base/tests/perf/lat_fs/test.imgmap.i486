/* Test image for constructor/metaconstructor. */
#define DEBUG

arch i486

/*********************************************
 * PRIMORDIAL DOMAINS
 *********************************************/

#include <primebank.map>
#include <pcc.map>
#include <metacon.map>
#include <vcs.map>
#include <nfile.map>

/********************************************************
 * Actual test case
 ********************************************************/

lat_fs = new process;

lat_fs.seg = program segment "lat_fs";
lat_fs.pc = symbol "lat_fs" _start;
lat_fs.seg = lat_fs.seg with page at 0x1f000;

lat_fs pc = lat_fs.pc;
lat_fs space = lat_fs.seg;
lat_fs schedule = sched(8);	/* normal */
/* lat_fs brand = start pcc 65535; */

/* lat_fs registers: */
lat_fs key reg 1 = nfilec;
lat_fs key reg 2 = lat_fs;
lat_fs key reg 3 = sched(8);
lat_fs key reg 4 = primebank;
lat_fs key reg 5 = misc Console;
lat_fs key reg 6 = misc SysTrace;
lat_fs key reg 7 = misc Sleep;
lat_fs key reg 11 = new node;

/* print domain lat_fs.dom */
hide lat_fs.seg;
hide lat_fs.pc;

run lat_fs;


print directory
