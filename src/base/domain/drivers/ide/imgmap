#include <primebank.map>
#include <pcc.map>
#include <metacon.map>
#include <vcs.map>
#include <ipltool.map>


MAKE_VIRTUAL_COPIER(ide_ide_vcsk, BUILDDIR "ide_ide");
BOOT_CONSTRUCTOR(ide_ide_c, ide_ide);
PROD_SPACE(ide_ide_c) = ide_ide_vcsk_c;
PROD_PC(ide_ide_c) = symbol BUILDDIR "ide_ide" _start;
PROD_CONSTIT(ide_ide_c, KC_OSTREAM, 3) = misc Console;
PROD_CONSTIT(ide_ide_c, KC_DEVICEPRIVS, 4) = misc DevicePrivs;
PROD_CONSTIT(ide_ide_c, KC_SLEEP, 6) = misc Sleep;

run ide_ide_c;

print directory;
