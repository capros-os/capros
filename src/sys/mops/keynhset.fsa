// node_ClearHazard() must be called before key_NH_Set()

state has_hazard "Has Hazard";
state no_hazard "No Hazard";
state error "Set key while hazardous";

ast clear_hazard { function_call { identifier "node_ClearHazard" } {
ellipsis } }; 
ast key_set { function_call { identifier "key_NH_Set" } { ellipsis }
};
ast other { other };

transition has_hazard no_hazard clear_hazard;
transition has_hazard error key_set;
transition has_hazard has_hazard other;

transition no_hazard has_hazard key_set;
transition no_hazard no_hazard other;

transition error error other;
